     name="make-house-public"
     permission="cmf.ModifyPortalContent"
     class=".helper.PublicHouseHelper"
     />

  <browser:page
     for="org.snsoffice.base.interfaces.ISpot"
     name="config.json"
     permission="zope2.Public"
