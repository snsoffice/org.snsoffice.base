  <browser:page
     name="geolocator"
     for="*"
     class=".helper.GeoLocator"
     template="geolocator.pt"
     layer="org.snsoffice.base.interfaces.IOrgSnsofficeBaseLayer"
     permission="zope2.View"
     />

